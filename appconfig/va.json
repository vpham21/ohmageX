      "history": "history",
      "reminder": "reminder",
      "profile": "profile",
      "logout": "logout"
    },
    "pages": {
      "login": "login",
      "dashboardeqis": "dashboard",
      "campaign": "campaigns",
      "survey": "surveys",
      "queue": "upload queue",
      "history": "history",
      "reminder": "reminders",
      "profile": "profile",
      "logout": "logout"
    },
    "menu": {
      "login": "Login",
      "dashboardeqis": "Dashboard",
      "campaign": "Campaigns",
      "survey": "Surveys",
      "queue": "Upload Queue",
      "history": "History",
      "reminder": "Reminders",
      "profile": "Profile",
      "logout": "Logout"
    }
  },
  "messages": {
    "reminder_body": "Tap to capture. Swipe to ignore."
  },
  "menu_items_disabled": {
    "native": [
      "dashboardeqis"
    ],
    "browser": [
      "dashboardeqis",
      "profile",
      "reminder",
      "logout"
    ],
    "footer": [
      "dashboardeqis",
      "campaign",
      "survey",
      "queue",
      "history",
      "reminder",
      "logout"
    ]
  },
  "routes": {
    "homepage": "campaign",
    "surveys": "survey",
    "help": "https://web.ohmage.org/ohmage/ohmagex-help.html"
  },
  "server_list": {
    "custom": false,
    "servers": [
      "https://test.mobilizingcs.org",
      "https://pilots.mobilizelabs.org"
    ]
  },
  "functionality": {
    "internal_links_enabled": false,
    "post_survey_reminders_disabled": true,
    "multi_question_survey_flow": false,
    "history_auto_refresh": false,
    "history_eqis_bucketing": false
  },
  "prompt_defaults": {
    "doc": {
      "max_bytes": 1000000000
    },
    "photo": {
      "max_pixels": 800
    },
    "video": {
      "max_seconds": 180,
      "caution_threshold_bytes": 300000000,
      "assumed_mb_per_minute": 90
    }
  },
  "user_preference_defaults": {
    "wifi_upload_only": false
  }
